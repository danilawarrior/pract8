        <Tab>
            <ShellContent Title="MainPage" ContentTemplate="{DataTemplate local:MainPage}" />
            <ShellContent Title="ListView" ContentTemplate="{DataTemplate local:ListView}" />
        </Tab>
    </TabBar>
</Shell>
